<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomRoundedButton">
        <attr name="buttonColor" format="color"/>
        <attr name="duration" format="integer"/>
        <attr name="text" format="string"/>
        <attr name="textColor" format="color"/>
        <attr name="textSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CustomLineChat">
        <attr name="color_line_above" format="color"/>
        <attr name="color_line_below" format="color"/>
        <attr name="color_gradient_above" format="color"/>
        <attr name="color_gradient_below" format="color"/>
    </declare-styleable>

</resources>